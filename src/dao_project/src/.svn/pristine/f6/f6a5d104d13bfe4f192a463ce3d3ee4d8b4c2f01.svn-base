package Presentacion.Contexto;

public class Contexto {

	private String evento;
	private Object datos;

	
	public Contexto(String evento, Object datos) {
		
		this.evento=evento;
		this.datos=datos;
	}


	/**
	 * Devuelve el evento
	 * @return
	 */
	public String getEvento() {
		
		return this.evento;
	}

	
	/**
	 * Devuelve los datos
	 * @return
	 */
	public Object getDatos() {
		
		return this.datos;
	}
	
}

